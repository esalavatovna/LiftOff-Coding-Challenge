function LongestWord(sen) { 

  // code goes here  


  var strSplit = sen.split(' ');
  var longestWord = "";
  for(var i = 0; i < strSplit.length; i++){
    if(strSplit[i].length > longestWord.length){
	longestWord = strSplit[i];
     }
  }

  return longestWord; 

}
   
// keep this function call here 
console.log(LongestWord(readline()));
